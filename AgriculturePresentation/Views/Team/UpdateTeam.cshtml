@model EntityLayer.Concrete.Team
@{
    ViewData["Title"] = "UpdateTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<div class="col-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Takım Personel Kaydı Güncelle</h4>

                  <form class="forms-sample" method="post">
                @Html.HiddenFor(x=>x.TeamId)
                    <div class="form-group">
                      <label for="exampleInputName1">İsim-Soyisim</label>
                      <input type="text" class="form-control" asp-for="PersonName" placeholder="İsim-Soyisim">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Görev</label>
                      <input type="text" class="form-control" asp-for="Title" placeholder="Görev">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Resim Url</label>
                      <input type="text" class="form-control" asp-for="ImageUrl" placeholder="Resim Url">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Facebook Url</label>
                      <input type="text" class="form-control" asp-for="FacebookUrl" placeholder="Facebook Url">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Twitter Url</label>
                      <input type="text" class="form-control" asp-for="TwitterUrl" placeholder="Twitter Url">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Instagram Url</label>
                      <input type="text" class="form-control" asp-for="InstagramUrl" placeholder="Instagram Url">
                    </div>

                    <div class="form-group">
                      <label for="exampleInputName1">Web Site Url</label>
                      <input type="text" class="form-control" asp-for="WebSiteUrl" placeholder="Web Site Url">
                    </div>

                    <button type="submit" class="btn btn-outline-success">Güncelle</button>

                  </form>
                </div>
              </div>
            </div>
